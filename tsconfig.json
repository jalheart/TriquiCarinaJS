{
  "compilerOptions": {
    "target": "ES6",
    "module": "system",
    "moduleResolution": "node",
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "declaration":true,
    "outDir":"./app"
  },
  "exclude": [
        "node_modules",
        "libs"        
  ]
}